<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background transparent -->
    <item android:drawable="@android:color/transparent"/>

    <!-- First line -->
    <item>
        <shape android:shape="rectangle">
            <size android:width="40dp" android:height="4dp"/> <!-- Width and height for the line -->
            <solid android:color="#FF6200EE"/> <!-- Color for the first line -->
            <padding android:left="8dp" android:right="8dp"/> <!-- Optional padding for aesthetics -->
        </shape>
    </item>

    <!-- Second line -->
    <item android:top="10dp"> <!-- Space between lines -->
        <shape android:shape="rectangle">
            <size android:width="40dp" android:height="4dp"/> <!-- Width and height for the line -->
            <solid android:color="#FF6200EE"/> <!-- Color for the second line -->
            <padding android:left="8dp" android:right="8dp"/> <!-- Optional padding for aesthetics -->
        </shape>
    </item>

    <!-- Third line -->
    <item android:top="10dp"> <!-- Space between lines -->
        <shape android:shape="rectangle">
            <size android:width="40dp" android:height="4dp"/> <!-- Width and height for the line -->
            <solid android:color="#FF6200EE"/> <!-- Color for the third line -->
            <padding android:left="8dp" android:right="8dp"/> <!-- Optional padding for aesthetics -->
        </shape>
    </item>
</layer-list>
